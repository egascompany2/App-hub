{
  "expo": {
    "name": "driver",
    "slug": "driver",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/egas-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#286EF0"
    },
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "googleMapsApiKey": "AIzaSyD-Vz-OkLGAeMiuT5_eUj2_V9x91FTrDM8"
      },
      "bundleIdentifier": "com.egascompany.driverapp",
      "entitlements": {
        "aps-environment": "production"
      },
      "podfileProperties": {
        "useModularHeaders": true,
        "useFrameworks": "static"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "2cc2-38-240-37-125.ngrok-free.app": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionMinimumTLSVersion": "1.0",
              "NSExceptionRequiresForwardSecrecy": false,
              "NSIncludesSubdomains": true
            }
          }
        },
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSUserNotificationUsageDescription": "We use notifications to alert you about new order assignments."
      }
    },
    "android": {
      "versionCode": 53,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/egas-logo.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.WAKE_LOCK",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.USE_FULL_SCREEN_INTENT"
      ],
      "package": "com.egas.driver",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD-Vz-OkLGAeMiuT5_eUj2_V9x91FTrDM8"
        }
      },
      "networkSecurityConfig": {
        "cleartextTrafficPermitted": true,
        "domainConfig": [
          {
            "domain": "2cc2-38-240-37-125.ngrok-free.app",
            "cleartextTrafficPermitted": true,
            "trustedCAs": []
          }
        ]
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-firebase/app",
        {
          "setAndroidName": true
        }
      ],
      "@react-native-firebase/messaging",
      "./plugins/withNotifee",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow location access for delivery tracking",
          "locationAlwaysPermission": "Allow location access for delivery tracking",
          "locationWhenInUsePermission": "Allow location access for delivery tracking",
          "isIosBackgroundLocationEnabled": true,
          "isAndroidBackgroundLocationEnabled": true
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "useModularHeaders": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5a8ed50e-4279-46be-8ae6-db1c2b828364"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "egasenergy31"
  }
}
