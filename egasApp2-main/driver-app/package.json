    "@types/jest": "^29.5.12",
    "@types/mapbox__polyline": "^1.0.5",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^18.0.7",
    "@types/socket.io-client": "^3.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.16",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
